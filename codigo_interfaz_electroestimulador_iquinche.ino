/////////////////////////////////////////////   ADC

void config_ADC(void){
  DIDR0 = 0x0F;   //deshabilito las entradas digitales correspondientes a los canales analgicos
  
              // 0100 0001 voltaje de ref = 5v, justificacin derecha, medida por el canal 1                          
  ADCSRA = 0xAC;           // 1010 1100 Habilito el conversor ADC, Habilita la interrupcion, INicia el preecalador en 16
  ADCSRB = 0x45;           // 0100 0101 AD channels MUX on, free running mode
  bitWrite(ADCSRA, 6, 1);  // Start the conversion by setting bit 6 (=ADSC) in ADCSRA
  sei();                   // set interrupt flag
  
  }

void config_timer1(void){
  TCCR1A = TCCR1B = 0;//ELIMINAR CUALQUIE CONF PREVIA DEL TIMER
  TCNT1 = 0;//Borro cualquier conteo o valor previo

  OCR1B = 1;//  SE COMPARA el timer 1 CONTRA 43750 CONTEOS...con el fin de obtener 700 ms
  TCCR1B |= (1 << WGM12);  //se configuro el timer 1 en modo de comparacin CTC
  TIMSK1 |= (1 << OCIE1B); //habilito la interrupcin por comparacin del timer 1 en el modo B.
  TCCR1B |= (1<<CS11);  //preescalador de 256
  TCCR1B |= (1<<CS10);  
  }
  /*** Interrupt routine ADC ready ***/
ISR(TIMER1_COMPB_vect) //Interrupt Service Routine
{
  TCNT1=0;
}
  
ISR(ADC_vect) {
  resul_adc = ADCL;        // store lower byte ADC
  resul_adc += ADCH << 8;  // store higher bytes ADC
}
 
void imprime_ADC(void){
  //solo imprime cuando el resultado de la conversin cambia
  if(resul_adc != result_adc_anterior){
    result_adc_anterior = resul_adc; 
  }
}

