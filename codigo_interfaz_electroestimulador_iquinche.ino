#define ANCHO 128     // reemplaza ocurrencia de ANCHO por 128
#define ALTO 64       // reemplaza ocurrencia de ALTO por 64

#define OLED_RESET 4      // necesario por la libreria pero no usado
Adafruit_SSD1306 oled(ANCHO, ALTO, &Wire, OLED_RESET);  // crea objeto

/////////////////////////////////////////////   ADC

int resul_adc, result_adc_anterior;

void imprime_ADC(void);
void config_ADC(void);    //funcin de configuracion del conversor ADC
void config_timer1(void); //funcion de configuracin del timer, el cual controla los disparos del ADC


void tiempo_main(void);
void ADCBzeroA2(void);


/////////////////////////////////////////////  PINES

int FR5 = 5;
int TMS6 = 6;

int enA = 8;
int enB = 9;

unsigned long T=0;
unsigned long TT=0;

int Bzero = 0;

/////////////////////////////////////
////////////////////////////////////////////////////  SETUP - LOOP
/////////////////////////////////////

void setup() {

  ///// ADC
  pinMode(A1, INPUT);
  pinMode(A2, INPUT);
  
  ///// OLED
  pinMode(A4, OUTPUT);
  pinMode(A5, OUTPUT);

  Serial.begin(9600);
  
  Wire.begin();
  oled.begin(SSD1306_SWITCHCAPVCC, 0x3C); // inicializa pantalla con direccion C=fisico D=simulador
  delay(100);
  oled.clearDisplay();      // limpia pantalla
  oled.display();

  ///// LECTURA FRE_MS
  pinMode(FR5, INPUT);
  pinMode(TMS6, INPUT);

  ///// MUX
  pinMode(enA, OUTPUT);
  pinMode(enB, OUTPUT);

  config_ADC();
  config_timer1();

  T = pulseIn(FR5, HIGH);
  TT = pulseIn(TMS6, HIGH);
    
}

